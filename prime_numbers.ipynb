        "# Finding the first n Prime numbers"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "SIXP4ZIHSnK5",
        "outputId": "d0d653d9-eaeb-4b4a-827f-e9a13189effa",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
      },
      "source": [
        "count = 10 #int(input(\"Enter count: \"))  \n",
        "n=0\n",
        "i=1\n",
        "while n<count:\n",
        "  j=1\n",
        "  ct=0\n",
        "  while j<=i:\n",
        "    if i%j==0:\n",
        "      ct=ct+1\n",
        "    j=j+1\n",
        "  if(ct==2):\n",
        "    print(i)\n",
        "    n=n+1\n",
        "  i=i+1"
      ],
      "execution_count": 1,
